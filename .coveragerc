[run]
source = .
omit = 
    tests/*
    */site-packages/*
    */__pycache__/*
    venv/*
    tvos/*
    .venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    
precision = 2

[html]
directory = htmlcov
